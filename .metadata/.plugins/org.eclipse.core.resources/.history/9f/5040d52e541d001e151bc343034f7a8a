
public class Jouney {

	public static void main(String[] args) {
		//Scanner scan = new Scanner(System.in);
		Jouney j = new Jouney();
		double d= j.calculateDistance(60.0, 1.5);
		System.out.println(d);
		
	}
	

	public double calculateDistance(double speed, double time)
	{
		return  speed*time;
		
	}


	}

}
